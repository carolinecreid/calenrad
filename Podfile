# platform :ios, '9.0'

target 'calenrad' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for calenrad
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'GoogleAPIClient/Calendar', '~> 1.0.2'
  pod 'GTMOAuth2', '~> 1.1.0'

  target 'calenradTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'calenradUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
